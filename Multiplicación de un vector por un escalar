Algoritmo MultiplicarPorEscalar

Definir vector Como Entero;
Definir escalar Como Entero;
Dimension vector[8];

Escribir "Ingrese los 8 n√∫meros del vector:";
Para i = 1 Hasta 8 Hacer
	Leer vector[i];
FinPara;

Escribir "Ingrese el valor escalar:";
Leer escalar;

Para i = 1 Hasta 8 Hacer
	vector[i] = vector[i] * escalar;
FinPara;

Escribir "El vector resultante es: ";
Para i = 1 Hasta 8 Hacer
	Escribir vector[i];
FinPara;
FinAlgoritmo
